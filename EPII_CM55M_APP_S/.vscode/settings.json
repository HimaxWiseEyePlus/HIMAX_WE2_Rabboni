{
    "cortex-debug.armToolchainPath": "/toolchain/arm-gnu-toolchain-13.2.Rel1-x86_64-arm-none-eabi/bin",
    "cortex-debug.gdbPath": "/toolchain/arm-gnu-toolchain-13.2.Rel1-x86_64-arm-none-eabi/bin/arm-none-eabi-gdb",
    "cortex-debug.variableUseNaturalFormat": true,
    "cortex-debug.registerUseNaturalFormat": true,
    "C_Cpp.errorSquiggles": "disabled",
    "C_Cpp.dimInactiveRegions": false,
    "C_Cpp.autoAddFileAssociations": false,
    "C_Cpp.default.compilerPath": "",
    "C_Cpp.default.systemIncludePath": [],
    "C_Cpp.default.cStandard": "gnu99",
    "C_Cpp.default.cppStandard": "c++11",
    "C_Cpp.default.defines": [],
    "C_Cpp.default.includePath": [],
    "C_Cpp.default.browse.path": [
        "/toolchain/arm-gnu-toolchain-13.2.Rel1-x86_64-arm-none-eabi/arm-none-eabi/include/**",
        "/toolchain/arm-gnu-toolchain-13.2.Rel1-x86_64-arm-none-eabi//lib/gcc/arm-none-eabi/13.2.1/**",
        "${workspaceFolder}/**"
    ],
    "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": true,
    "C_Cpp.default.browse.databaseFilename": "",
    "C_Cpp.default.intelliSenseMode": "gcc-arm",
    "C_Cpp.intelliSenseCachePath": "${workspaceFolder}/.vscode/browse.vc.db",
    "C_Cpp.intelliSenseCacheSize": 4,
    "C_Cpp.intelliSenseEngine": "Tag Parser",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "search.followSymlinks": false,
    "git.autorefresh": false,
    "editor.formatOnSave": false,
    "editor.mouseWheelZoom": true,
    "editor.smoothScrolling": false,
    "editor.cursorSmoothCaretAnimation": "off",
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "files.autoSave": "off",
    "files.exclude": {
        "**/.project": true,
        "**/.cproject": true,
        "**/.pydevproject": true,
        "**/.checkstyle": true,
        "**/.settings": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.idea": true,
        "**/*.spec": true,
        "**/*.pytest_cache": true,
        "**/*.pyc": true,
        "**/__pycache__": true,
        "**/*.cache": true,
        "**/.DS_Store": true,
        "**/*.db": true,
        "**/*.db-wal": true,
        "**/*.db-shm": true,
        "**/*.bak": true
    },
    "cortex-debug.serverTimeout": 500000
}